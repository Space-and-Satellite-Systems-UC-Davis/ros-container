{
  "id": "ros-nav2",
  "version": "1.0.0",
  "name": "ROS 2 + Nav2 Development Container",
  "description": "ROS 2 development environment with Nav2 navigation stack, Gazebo Harmonic, and VNC desktop access",
  "publisher": "Space-and-Satellite-Systems-UC-Davis",
  "documentationURL": "https://github.com/Space-and-Satellite-Systems-UC-Davis/ros-container",
  "licenseURL": "https://github.com/Space-and-Satellite-Systems-UC-Davis/ros-container/blob/main/LICENSE",
  "platforms": ["linux/amd64", "linux/arm64"],
  "keywords": ["ros", "ros2", "nav2", "gazebo", "robotics", "navigation"],
  "options": {
    "rosDistro": {
      "type": "string",
      "description": "ROS 2 distribution to use",
      "proposals": ["kilted", "jazzy", "humble"],
      "default": "jazzy"
    }
  },
  "ltsRelease": "jazzy"
}
